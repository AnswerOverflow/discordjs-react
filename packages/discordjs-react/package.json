{
  "name": "@answeroverflow/discordjs-react",
  "version": "0.0.28",
  "main": "./index.ts",
  "dist-main": "./dist/index.js",
  "types": "./index.ts",
  "dist-types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npx main-updater@latest prepublish && pnpm build",
    "postpublish": "npx main-updater@latest postpublish",
    "lint": "eslint \"**/*.ts*\"",
    "build": "tsup index.ts --target node16 --format cjs,esm --dts --sourcemap",
    "build-watch": "pnpm build -- --watch",
    "clean": "rm -rf node_modules dist"
  },
  "peerDependencies": {
    "discord.js": "^14",
    "react": ">=18"
  },
  "dependencies": {
    "@answeroverflow/discordjs-mock": "0.0.12",
    "react-reconciler": "^0.29.0",
    "rxjs": "^7.8.0"
  },
  "overrides": {
    "discord-api-types": "0.37.20"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "*",
    "@types/react": "^18.0.28",
    "@types/react-reconciler": "^0.28.0",
    "discord.js": "^14.7.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.6.3",
    "type-fest": "3.5.7",
    "typescript": "^4.5.2"
  },
  "dist-exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": {
      "import": "./package.json",
      "require": "./package.json"
    }
  }
}
