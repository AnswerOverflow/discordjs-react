{
    "name": "@answeroverflow/discordjs-react",
    "version": "0.0.7",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "private": false,
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./package.json": {
            "import": "./package.json",
            "require": "./package.json"
        }
    },
    "scripts": {
        "prepublish": "pnpm build",
        "lint": "eslint \"**/*.ts*\"",
        "build": "tsup index.ts --target node16 --format cjs,esm --dts --sourcemap",
        "build-watch": "pnpm build -- --watch",
        "clean": "rm -rf node_modules dist"
    },
    "peerDependencies": {
        "discord.js": "^14",
        "react": ">=18"
    },
    "dependencies": {
        "@types/node": "*",
        "@types/react": "*",
        "@types/react-reconciler": "^0.28.0",
        "react-reconciler": "^0.29.0",
        "rxjs": "^7.8.0",
        "type-fest": "3.5.7"
    },
    "overrides": {
        "discord-api-types": "0.37.20"
    },
    "devDependencies": {
        "@types/lodash-es": "^4.17.6",
        "@types/react": "^17.0.37",
        "discord.js": "^14.7.1",
        "eslint": "^7.32.0",
        "eslint-config-custom": "workspace:*",
        "lodash-es": "^4.17.21",
        "react": "^18.2.0",
        "tsconfig": "workspace:*",
        "tsup": "^6.6.3",
        "typescript": "^4.5.2"
    }
}
