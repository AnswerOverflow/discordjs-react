{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "lint": {},
    "dev": {
      "dependsOn": [
        "build"
      ],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}